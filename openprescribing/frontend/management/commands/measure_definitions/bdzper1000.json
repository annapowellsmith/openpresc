{
  "name": "Anxiolytics and Hypnotics: Average Daily Quantity per patient",
  "title": [
    "Anxiolytics and Hypnotics: Average Daily Quantity per item per 1000 patients"
  ],
  "description": [
    "Number of average daily quantities (ADQs) per 1000 patients"
  ],
  "why_it_matters": [
    "<tbc when i've seen the data"
  ],
  "numerator_short": "Anxiolytics and Hypnotics ADQ",
  "denominator_short": "1000 Patients",
  "url": null,
  "is_percentage": false,
  "is_cost_based": false,
  "low_is_good": true,
  "tags": ["core"],
  "numerator_columns": [
    "SUM(p.quantity * r.percent_of_adq) AS numerator, "
  ],
  "numerator_from": "{hscic}.normalised_prescribing_standard p  LEFT JOIN {hscic}.bdz_adq r  ON concat(substr(p.bnf_code,0,9),substr(p.bnf_code,-2)) = concat(substr(r.bnf_code,0,9),substr(r.bnf_code,-2)) ",
  "numerator_where": [
    "(p.bnf_code like '0401%'",
		" AND p.bnf_code not like '0401010AD%')",
		" AND p.bnf_code NOT LIKE '0401010AC%'",
		" AND p.bnf_code NOT LIKE '0401020K0%AD'",
		" AND p.bnf_code NOT LIKE '0401020K0%AE'",
		" AND p.bnf_code NOT LIKE '0401020K0%BQ')"

  ],
  "denominator_columns": [
    "SUM(total_list_size / 1000.0) AS denominator, "
  ],
  "denominator_from": "{hscic}.practice_statistics ",
  "denominator_where": [
    "1 = 1"
  ]
}
